:2200409	
	    rdfs:label "多嚢胞性卵巣症候群"@ja;
	    nando:hasNotificationNumber "63";
	    skos:prefLabel "多嚢胞性卵巣症候群"@ja;
	    rdfs:seeAlso <https://www;kegg;jp/dbget-bin/www_bget?ds_ja:H01739>;
	    rdfs:subClassOf nando:2100149;
