:1200437	
	    rdfs:label "バッド・キアリ症候群"@ja;
	    nando:hasNotificationNumber "91";
	    skos:prefLabel "バッド・キアリ症候群"@ja;
	    rdfs:seeAlso <https://www;kegg;jp/dbget-bin/www_bget?ds_ja:H01433>;
	    rdfs:subClassOf nando:1100013;
	    rdf:type owl:Class;
