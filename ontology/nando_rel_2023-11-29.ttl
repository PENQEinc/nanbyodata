:1200698	
	    rdfs:label "修正大血管転位症"@ja;
	    nando:hasNotificationNumber "208";
	    skos:prefLabel "修正大血管転位症"@ja;
	    rdfs:seeAlso <https://www;kegg;jp/dbget-bin/www_bget?ds_ja:H01786>;
	    rdfs:subClassOf nando:1100005;
	    rdf:type owl:Class;
