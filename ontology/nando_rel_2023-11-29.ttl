:1200673	
	    rdfs:label "多脾症候群"@ja;
	    nando:hasNotificationNumber "188";
	    skos:prefLabel "多脾症候群"@ja;
	    rdfs:seeAlso <https://www;kegg;jp/dbget-bin/www_bget?ds_ja:H01764>;
	    rdfs:subClassOf nando:1100014;
	    rdf:type owl:Class;
