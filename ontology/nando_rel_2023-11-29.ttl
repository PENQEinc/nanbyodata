:1200473	
	    rdfs:label "非典型溶血性尿毒症症候群"@ja;
	    nando:hasNotificationNumber "109";
	    skos:prefLabel "非典型溶血性尿毒症症候群"@ja;
	    rdfs:seeAlso <https://www;kegg;jp/dbget-bin/www_bget?ds_ja:H01434>;
	    rdfs:subClassOf nando:1100007;
	    rdf:type owl:Class;
