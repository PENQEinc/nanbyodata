:2200122	
	    rdfs:label "メサンギウム増殖性糸球体腎炎"@ja;
	    nando:hasNotificationNumber "43";
	    skos:prefLabel "メサンギウム増殖性糸球体腎炎"@ja;
	    rdfs:subClassOf nando:2100010;
	    rdf:type owl:Class;
