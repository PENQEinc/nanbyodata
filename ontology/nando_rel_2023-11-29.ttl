	    skos:prefLabel "軟骨無形成症"@ja;
	    rdfs:subClassOf nando:2100293;
	    rdf:type owl:Class;
	    rdfs:seeAlso <http://syndromefinder;ncchd;go;jp/UR-DBMS/SyndromeDetail;php?winid=1&recid=8>;
