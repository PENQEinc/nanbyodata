:2200254	
	    rdfs:label "ファロー四徴症"@ja;
	    nando:hasNotificationNumber "89";
	    skos:altLabel "Fallot四徴症"@ja;
	    skos:prefLabel "ファロー四徴症"@ja;
	    rdfs:subClassOf nando:2100075;
