	    rdf:type owl:Class;
	    rdfs:seeAlso nando:2201255;
	    dcterms:source <https://www;nanbyou;or;jp/wp-content/uploads/upload_files/File/079-201704-kojin;pdf>;
	    skos:altLabel "Homozygous familial hypercholesterolaemia"@en;
	    skos:altLabel "Homozygous FH"@en;
