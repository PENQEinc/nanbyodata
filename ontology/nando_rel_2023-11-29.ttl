:1200953	
	    rdfs:label "進行性ミオクローヌスてんかん"@ja;
	    nando:hasNotificationNumber "309";
	    skos:prefLabel "進行性ミオクローヌスてんかん"@ja;
	    rdfs:seeAlso <https://www;kegg;jp/dbget-bin/www_bget?ds_ja:H00810>;
