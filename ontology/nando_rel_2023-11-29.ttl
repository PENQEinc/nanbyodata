:2200638	
	    rdfs:label "微小血管障害性溶血性貧血"@ja;
	    nando:hasNotificationNumber "47";
	    skos:prefLabel "微小血管障害性溶血性貧血"@ja;
	    rdfs:subClassOf nando:2100185;
