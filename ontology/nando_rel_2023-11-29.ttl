:2200327	
	    rdfs:label "中枢性塩喪失症候群"@ja;
	    nando:hasNotificationNumber "67";
	    skos:prefLabel "中枢性塩喪失症候群"@ja;
	    rdfs:subClassOf nando:2100118;
