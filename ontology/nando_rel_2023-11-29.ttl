:1200881	
	    rdfs:label "巨大リンパ管奇形（頚部顔面病変）"@ja;
	    nando:hasNotificationNumber "278";
	    skos:prefLabel "巨大リンパ管奇形（頚部顔面病変）"@ja;
	    rdfs:subClassOf nando:1100010;
